<h1 align='center'>Search</h1>
<%= form_with(url: search_path, method: :get, local: true) do |f| %>

  <div class='field'>
    <%= f.label :start_station %>
    <%= f.collection_select :start_id, RailwayStation.all, :id, :title %>
  </div>

  <div class='field'>
    <%= f.label :end_station %>
    <%= f.collection_select :end_id, RailwayStation.all, :id, :title %>
  </div>

  <div class'actions'>
    <%= f.submit 'Search' %>
  </div>
<% end %>
<p><%= link_to 'Back to main', welcome_index_path %></p>
