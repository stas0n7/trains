<tr>
  <td>
    <div class="title_<%= railway_station.id %>">
      <%= railway_station.title %>
    </div>
    <%= render 'form', station: railway_station %>
  </td>
  <td><%= link_to t('common.show'), [:admin, railway_station] %></td>
  <td><%= link_to t('common.edit'), '', class: 'edit_link', data: { station: railway_station.id} %></td>
  <td><%= link_to t('common.destroy'), [:admin, railway_station], method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
