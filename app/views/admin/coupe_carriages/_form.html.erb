<%= form_with(model: [:admin,@train, @coupe_carriage], local: true) do |form| %>
  <%= render 'common/errors', resource: @coupe_carriage %>


  <div class="field">
    <%= form.label :top_seats %><br>
    <%= form.text_field :top_seats, class: 'form-control col-3' %>
  </div>
  <br>
  <div class="field">
    <%= form.label :bottom_seats %><br>
    <%= form.text_field :bottom_seats, class: 'form-control col-3' %>
  </div>
  <br>
  <div class="actions">
    <%= form.submit class: 'btn btn-primary' %>
  </div>
<% end %>
